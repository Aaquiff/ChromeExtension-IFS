{
  "manifest_version": 2,

  "name": "Search JIRA",
  "description": "This extension allows to search JIRA",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click To Search!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "tabs"
  ]
}
